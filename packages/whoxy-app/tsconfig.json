{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "skipLibCheck": true,
    "lib": ["ESNext"],
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "declaration": true,
    "outDir": "dist",
    "rootDir": "src",
    "composite": true,
    "types": ["./worker-configuration.d.ts"]
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist"]
}
